{
  "name": "typescript-loadable-ssr-example",
  "version": "1.0.0",
  "description": "Typescript + @loadable/component + Server Side Rendering + Webpack4 Example",
  "scripts": {
    "build": "npm run build:clean && npm run build:client && npm run build:server && npm run build:lib",
    "build:dev": "NODE_ENV=development npm run build",
    "build:clean": "rimraf ./dist",
    "build:client": "webpack --config ./build/webpack.client.js",
    "build:server": "webpack --config ./build/webpack.server.js",
    "build:lib": "webpack --config ./build/webpack.lib.js",
    "start": "npm run build && node dist/node/server.js",
    "start:dev": "NODE_ENV=development node dist/node/server.js",
    "start:production": "NODE_ENV=production npm run start",
    "start:development": "NODE_ENV=development npm run start"
  },
  "author": "Hacksign",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@loadable/component": "^5.13.1",
    "@loadable/server": "^5.13.1",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@loadable/babel-plugin": "^5.13.0",
    "@loadable/webpack-plugin": "^5.13.0",
    "@types/express": "^4.17.7",
    "@types/loadable__component": "^5.13.0",
    "@types/loadable__server": "^5.12.2",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.21",
    "babel-loader": "^8.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
